{
  "name": "log-cleaner",
  "version": "1.0.0",
  "description": "Administrate logs",
  "main": "index.ts",
  "bin": "./build/index.js",
  "scripts": {
    "start": "ts-node --transpile-only ./src/index.ts",
    "build": "tsc",
    "build-check": "tsc --noEmit",
    "pkg": "pkg ./build/"
  },
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "bin"
  },
  "repository": "https://github.com/jmveigac/log-cleaner",
  "keywords": [],
  "author": "jmveigac",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.6",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}